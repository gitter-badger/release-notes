@{
    ViewData["Title"] = "About PressRelease";
}
<h2 class="page-header">@ViewData["Title"]</h2>

<div class="container">
    <div class="row">
        <div class="lead col-md-3">
            <p><strong>PressRelease</strong> is a utility borne of frustration and curiosity. We hope that you find it useful.</p>
            <p>If you find a bug, have a suggestion, or want to contribute to the project, please check out <a href="https://github.com/somewhatabstract/PressRelease">our GitHub repository</a>.</p>
        </div>
        <div class="col-md-9">
            <div class="panel panel-default">
                  <div class="panel-heading">
                        <h3 class="panel-title">Release Notes</h3>
                  </div>
                  <div class="panel-body">
                        Panel content
                  </div>
            </div>
        </div>
    </div>
</div>

